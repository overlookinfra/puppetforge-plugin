<?xml version="1.0" encoding="UTF-8"?>
<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <f:entry field="circularDependency" title="${%Circular module dependencies}">
    <f:select default="${descriptor.defaults.circularDependency}"/>
  </f:entry>
  <f:entry field="dependencyVersionMismatch" title="${%Dependency Version Mismatch}">
    <f:select default="${descriptor.defaults.dependencyVersionMismatch}"/>
  </f:entry>
  <f:entry field="deprecatedKey" title="${%Deprecated metadata key}">
    <f:select default="${descriptor.defaults.deprecatedKey}"/>
  </f:entry>
  <f:entry field="missingForgeRequiredFields" title="${%Missing fields required by Puppet Forge}">
    <f:select default="${descriptor.defaults.missingForgeRequiredFields}"/>
  </f:entry>
  <f:entry field="moduleClassNotInInitPP" title="${%Module class not defined in manifests/init.pp}">
    <f:select default="${descriptor.defaults.moduleClassNotInInitPP}"/>
  </f:entry>
  <f:entry field="moduleNameNotStrict" title="${%Module Name not Strictly Correct}">
    <f:select default="${descriptor.defaults.moduleNameNotStrict}"/>
