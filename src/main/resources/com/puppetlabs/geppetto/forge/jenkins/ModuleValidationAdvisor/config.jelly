  <f:entry field="unexpectedSubmodule" title="${%Unexpected Submodule Directory}">
    <f:select default="${descriptor.defaults.unexpectedSubmodule}"/>
  </f:entry>
  <f:entry field="unrecognizedKey" title="${%Unrecognized metadata key}">
    <f:select default="${descriptor.defaults.unrecognizedKey}"/>
  </f:entry>
  <f:entry field="unresolvedReference" title="${%Unresolved Module Reference}">
    <f:select default="${descriptor.defaults.unresolvedReference}"/>
  </f:entry>
  <f:entry field="whitespaceInTag" title="${%Tag Contains Whitespace}">
    <f:select default="${descriptor.defaults.whitespaceInTag}"/>
  </f:entry>
</j:jelly>
